{% extends "base.html" %}
{% block content %}

<div id="wrappererr">
<div class="standard_error">
	<h2 class="blockhead">Notice from Administrator</h2>
	<form class="block vbform" method="post" action="/login">
		<div class="blockbody formcontrols">
			<h3 class="blocksubhead">Login</h3>
			<div class="blockrow">
				<label for="">Name:</label>
				<input type="text" class="primary textbox" id="" name="username" tabindex="1" />
			</div><!-- end blockrow -->
			<div class="blockrow">
				<label for="">Password:</label>
				<input type="password" class="primary textbox" id="" name="password" tabindex="1" />				
			</div><!-- end blockrow -->
			<div>
				<input type="checkbox" name="" id="" value="" tabindex="1" >Remember?</input>
			</div><!-- single check -->
		</div><!-- end blockbody form -->
		<div class="blockfoot actionbuttons">
			<div class="group">
				<input type="submit" class="" value="Login" tabindex="1" />
				<input type="reset" class="" value="Cancel" tabindex="1" />
			</div><!-- end group -->
		</div><!-- button -->
	</form>
</div><!-- end error -->
</div><!-- end wrapper -->

{% endblock %}